	<parameter name="remittance_id" class="java.lang.Integer"/>
	<parameter name="timezone" class="java.lang.String">
		<defaultValueExpression><![CDATA["Asia/Manila"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
		rmt.id,
		rmt.source_reference_number,
		rmd.remitter_name,
		rmd.remitter_address,
		rmd.remitter_contact_number,
		rmd.remitter_id_type,
		rmd.remitter_id_number,
		rmd.beneficiary_name,
		rmd.beneficiary_address,
		rmd.beneficiary_contact_number,
		rmd.beneficiary_id_type,
		rmd.beneficiary_id_number,
		rmd.payout_amount,
		rmd.handling_fee,
		rmd.date_created::TIMESTAMP AT TIME ZONE 'UTC' AT TIME ZONE $P{timezone} as date_created,
		rmd.payout_amount + rmd.handling_fee as total_fee,
		wb.name
FROM
		web_remittance rmt LEFT OUTER JOIN web_branch wb ON rmt.source_branch_id = wb.id,
		(
			SELECT
					DISTINCT ON	(remittance_id)
					remittance_id, 
					remitter_name,
					remitter_address,
					remitter_contact_number,
					remitter_id_type,
					remitter_id_number,
					beneficiary_name,
					beneficiary_address,
					beneficiary_contact_number,
					beneficiary_id_type,
					beneficiary_id_number,
					payout_amount,
					handling_fee,
					date_created
			FROM
					web_remittancedetail
			WHERE
					status='NEW'
			ORDER BY
					remittance_id, date_created DESC
		)rmd
WHERE
		rmt.id = $P{remittance_id}
		AND rmt.id = rmd.remittance_id]]>
	</queryString>
