	<parameter name="start_date" class="java.util.Date"/>
	<parameter name="end_date" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT DISTINCT 
    wr.remitter_id,
    wr.remitter_first_name,
    wr.remitter_middle_name,
    wr.remitter_last_name,
	wr.remitter_address,
	wr.remitter_contact_number,
	wr.payout_amount,
	r.source_reference_number,
	r.cashin_date	 
FROM
   (
   	SELECT DISTINCT ON (wr.remittance_id)
   	   wr.remittance_id,
   	    wr.remitter_id,
   	    upper(wr.remitter_first_name) as remitter_first_name,
   	    upper(wr.remitter_middle_name) as remitter_middle_name,
   	    upper(wr.remitter_last_name) as remitter_last_name,
   	    upper(wr.remitter_address) as remitter_address,
   	    wr.remitter_contact_number,
   	    wr.payout_amount,
   	    wr.status
   	 FROM
   	     web_remittancedetail wr
   )wr JOIN web_remittance r ON wr.remittance_id = r.id
WHERE
   CAST(r.cashin_date::TIMESTAMP AT TIME ZONE 'UTC' AT TIME ZONE 'PHT' as DATE)
   BETWEEN $P{start_date} AND $P{end_date}
   AND wr.status != 'CANCELLED' 
   AND wr.status != 'FRAUD'
   AND wr.status != 'APPROVED_CANCELLATION'
ORDER BY
    wr.remitter_last_name, wr.remitter_first_name, wr.remitter_middle_name, r.cashin_date
    ]]>
	</queryString>
